# Which search service to use: 'duckduckgo', 'tavily', 'perplexity', 'searxng' (searxng includes arXiv results)
SEARCH_API='searxng'
# For Searxng search, defaults to http://localhost:8888
SEARXNG_URL=http://localhost:8001/

# Web Search API Keys (choose one or both)
TAVILY_API_KEY=tvly-xxxxx      # Get your key at https://tavily.com
PERPLEXITY_API_KEY=pplx-xxxxx  # Get your key at https://www.perplexity.ai

# LLM Configuration
LLM_PROVIDER=openai_compatible    # Options: ollama, openai_compatible
LOCAL_LLM=MonkeyResearcher  # Model name in your provider
OLLAMA_BASE_URL=http://localhost:11434  # the endpoint of the Ollama service, defaults to http://localhost:11434 if not set
OPENAI_COMPATIBLE_BASE_URL=http://localhost:22222/v1  # OpenAI-compatible API URL
OPENAI_COMPATIBLE_API_KEY=EMPTY  # API key for OpenAI-compatible server

MAX_WEB_RESEARCH_LOOPS=3
FETCH_FULL_PAGE=True
LANGSMITH_API_KEY=lsv2_...
